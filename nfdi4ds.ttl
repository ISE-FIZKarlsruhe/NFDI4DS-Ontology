cono:OrganizedEvent rdf:type owl:Class ;
                    rdfs:subClassOf nfdicore:Event ;
                    obo:IAO_0000115 "Any kind of organised event having a start and an end date."@en ;
                    rdfs:isDefinedBy <http://www.scholarlydata.org/ontology/doc/#d4e1203> ;
                    rdfs:label "organized event" ;
                    <http://www.w3.org/2004/02/skos/core#example> "conferences, workshops, lectures, or any other planned event."@en .


###  https://w3id.org/scholarlydata/ontology/conference-ontology.owl#Tutorial
cono:Tutorial rdf:type owl:Class ;
              rdfs:subClassOf cono:AcademicEvent ;
              rdfs:comment "A tutorial event."@en ;
              rdfs:label "tutorial" .


###  https://w3id.org/scholarlydata/ontology/conference-ontology.owl#Workshop
cono:Workshop rdf:type owl:Class ;
