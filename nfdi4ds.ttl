###  https://nfdi.fiz-karlsruhe.de/ontology/Storage&Preservation
<https://nfdi.fiz-karlsruhe.de/ontology/Storage&Preservation> rdf:type owl:Class ;
                                                              rdfs:subClassOf nfdicore:Service ;
                                                              obo:IAO_0000115 "A service that contributes to long-term archiving and data integrity through secure storage and preservation strategies."@en ;
                                                              rdfs:comment "Services that focus on the long-term archiving and preservation of research data and digital assets. This involves providing secure storage infrastructures, backup solutions, and preservation strategies to prevent data loss over time."@en ;
                                                              rdfs:isDefinedBy <https://nfdi.fiz-karlsruhe.de/ontology/2.0.0> ;
                                                              rdfs:label "storage & preservation"@en ;
                                                              <http://www.w3.org/2004/02/skos/core#example> "RADAR4Culture, Curated Repository List"@en .


###  https://nfdi4ds.de/ontology/EducationalFunction
:EducationalFunction rdf:type owl:Class ;
                     rdfs:subClassOf obo:BFO_0000034 ;
                     obo:IAO_0000115 "Educational function is \"One of four basic kinds of uses of social communication listed by Schramm from the perspective of the individual, in which the normative objectives are for the sender to teach and for the receiver to learn. He distinguishes this from a purely informational function. From the  perspective of society, Schramm argues that this function is analogous  to the function of socialization. See also media functions; personal functions.\""@en ;
                     obo:IAO_0000119 "https://www.oxfordreference.com/display/10.1093/oi/authority.20110803095742736" ;
                     rdfs:label "educational function" .


###  https://nfdi4ds.de/ontology/EvaluationMetric
:EvaluationMetric rdf:type owl:Class ;
                  rdfs:subClassOf obo:IAO_0000003 ;
                  rdfs:comment "A quantitative measure used to assess the performance and effectiveness of a statistical or machine learning model."@en ;
                  rdfs:label "evaluation metric" .


###  https://nfdi4ds.de/ontology/Hackathon
:Hackathon rdf:type owl:Class ;
           rdfs:subClassOf cono:OrganizedEvent ;
           obo:IAO_0000115 "hackathon" ;
           rdfs:comment "An event where individuals or teams come together to engage in rapid and collaborative engineering over a relatively short period of time."@en .


###  https://nfdi4ds.de/ontology/Lecture
:Lecture rdf:type owl:Class ;
         rdfs:subClassOf cono:OrganizedEvent ;
         obo:IAO_0000115 "An academic event which is intented to deliver instruction on a specific topic, most commonly to an audience of university students."@en ;
         rdfs:label "Lecture" .


###  https://nfdi4ds.de/ontology/NFDI4DSJointProject
:NFDI4DSJointProject rdf:type owl:Class ;
                     rdfs:subClassOf [ owl:intersectionOf ( nfdicore:Project
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty obo:RO_0000057 ;
                                                              owl:hasValue :NFDI4DS_Consortium
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] .


###  https://nfdi4ds.de/ontology/NFDI4DSSubProject
:NFDI4DSSubProject rdf:type owl:Class ;
                   rdfs:subClassOf nfdicore:Project .


###  https://nfdi4ds.de/ontology/ScienceSlam
:ScienceSlam rdf:type owl:Class ;
             rdfs:subClassOf cono:OrganizedEvent ;
             rdfs:comment "A scientific talk where scientists present their own scientific research work in a given time frame in front of a non-expert audience."@en ;
             rdfs:label "science slam" ;
             <http://www.w3.org/2004/02/skos/core#example> "2024 NFDI Science Slam" .


###  https://nfdi4ds.de/ontology/SharedTask
:SharedTask rdf:type owl:Class ;
            rdfs:subClassOf cono:AcademicEvent ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :eventOrganizer ;
                              owl:someValuesFrom [ owl:intersectionOf ( nfdicore:ConsortiumMember
                                                                        [ rdf:type owl:Restriction ;
                                                                          owl:onProperty nfdicore:member ;
                                                                          owl:hasValue :NFDI4DS_Consortium
                                                                        ]
                                                                      ) ;
                                                   rdf:type owl:Class
                                                 ]
                            ] ;
            rdfs:comment "An interactive event organized by NFDI4DS that attract the data science and AI community. The task may be divided into subtasks."@en ;
            rdfs:label "shared task" ;
            <http://www.w3.org/2004/02/skos/core#example> "Scholarly QALD (Question Answering over Scholarly Knowledge Graphs) at ISWC 2023 https://kgqa.github.io/scholarly-QALD-challenge/2023/"@en .


###  https://nfdi4ds.de/ontology/SubTask
:SubTask rdf:type owl:Class ;
         rdfs:subClassOf cono:AcademicEvent ;
         rdfs:comment "One of the divided tasks of a certain shared task."@en ;
         rdfs:label "sub task" ;
         <http://www.w3.org/2004/02/skos/core#example> "Task 1: DBLP-QUAD — Knowledge Graph Question Answering over DBLP https://kgqa.github.io/scholarly-QALD-challenge/2023/" ,
                                                       "Task 2: SciQA — Question Answering of Scholarly Knowledge" .


###  https://nfdi4ds.de/ontology/SummerSchool
:SummerSchool rdf:type owl:Class ;
              rdfs:subClassOf cono:OrganizedEvent ;
              rdfs:comment "An intensive summer educational program with a focus on specialized topics within a particular field or discipline to offer participants the opportunity to deepen their knowledge, develop new skills, and engage with experts and peers in the field."@en ;
              rdfs:label "summer school" ;
              <http://www.w3.org/2004/02/skos/core#example> "International Semantic Web Summer School 2024 (ISWS) https://2024.semanticwebschool.org/"@en .


